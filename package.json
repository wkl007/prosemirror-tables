{
  "name": "prosemirror-tables",
  "version": "1.1.1",
  "description": "ProseMirror's rowspan/colspan tables component",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "style": "style/tables.css",
  "typings": "./index.d.ts",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/prosemirror/prosemirror-tables.git"
  },
  "maintainers": [
    {
      "name": "Eduard Shvedai",
      "email": "eshvedai@atlassian.com"
    },
    {
      "name": "Huanhuan Huang",
      "email": "hhuang@atlassian.com"
    }
  ],
  "scripts": {
    "build": "rollup -c",
    "watch": "rollup -w -c",
    "build_readme": "builddocs --name tables --format markdown --main src/README.md src/*.js > README.md",
    "build_demo": "rollup -c rollup.demo.config.js",
    "watch_demo": "rollup -w -c rollup.demo.config.js",
    "lint": "npm run lint:code && npm run lint:style",
    "lint:code": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint:style": "stylelint --fix style/**/*.{css,less,scss,sass} ",
    "test": "mocha test/test-*.js",
    "typecheck": "tsc --noEmit",
    "prepare": "husky install"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": "prettier --write",
    "style/**/*.{css,less,scss,sass}": "stylelint --fix"
  },
  "dependencies": {
    "prosemirror-keymap": "^1.1.5",
    "prosemirror-model": "^1.16.1",
    "prosemirror-state": "^1.3.4",
    "prosemirror-transform": "^1.3.3",
    "prosemirror-view": "^1.23.5"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.16.5",
    "@babel/plugin-transform-runtime": "^7.16.7",
    "@commitlint/cli": "^16.0.1",
    "@commitlint/config-conventional": "^16.0.0",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-buble": "^0.21.3",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.1.2",
    "@types/prosemirror-model": "^1.13.2",
    "@types/prosemirror-state": "^1.2.8",
    "builddocs": "^1.0.1",
    "eslint": "^8.6.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.3",
    "husky": "^7.0.4",
    "ist": "^1.1.7",
    "lint-staged": "^12.1.4",
    "mocha": "^9.1.3",
    "prettier": "^2.5.1",
    "prosemirror-commands": "^1.1.12",
    "prosemirror-example-setup": "^1.1.2",
    "prosemirror-menu": "^1.1.4",
    "prosemirror-schema-basic": "^1.1.2",
    "prosemirror-test-builder": "^1.0.5",
    "rollup": "^2.62.0",
    "stylelint": "^14.2.0",
    "stylelint-config-html": "^1.0.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^24.0.0",
    "stylelint-order": "^5.0.0",
    "typescript": "^4.5.4",
    "typings-tester": "^0.3.2"
  }
}
